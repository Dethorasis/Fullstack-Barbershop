{
  "name": "boilerplate-fullstack",
  "version": "2.0.0",
  "main": "server/index.js",
  "license": "MIT",
  "keywords": [
    "fullstack"
  ],
  "scripts": {
    "start": "ts-node server/index.ts",
    "test": "jest",
    "test:watch": "jest --noStackTrace --watchAll",
    "dev": "run-p dev:client dev:server tailwind",
    "dev:client": "npm run webpack -- --watch",
    "dev:server": "nodemon server/index.ts",
    "build": "run-p build:client build:server",
    "build:client": "npm run webpack -- --mode production",
    "build:server": "npm run knex -- migrate:latest",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "webpack": "webpack --config ./client/webpack.config.js",
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "tailwind": "npx tailwindcss -i ./server/public/input.css -o ./server/public/output.css --watch"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "@devacademy/eslint-config/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.1",
    "@redux-devtools/extension": "^3.2.3",
    "bcrypt": "^5.1.1",
    "express": "^4.17.2",
    "express-session": "^1.17.3",
    "knex": "^2.3.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.8.0",
    "sqlite3": "^5.1.2",
    "superagent": "7.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.17.12",
    "@devacademy/eslint-config": "^1.8.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/react": "^13.4",
    "@types/express": "^4.17.14",
    "@types/multer": "^1.4.8",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@types/superagent": "^4.1.17",
    "babel-loader": "^8.2.2",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.3.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "react": "^18.0.2",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "supertest": "^6.2.2",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom"
  },
  "browserslist": "> 2%, not dead"
}
